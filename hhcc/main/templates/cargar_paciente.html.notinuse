<!doctype html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cargar Paciente</title>
</head>

<body>
    {% if paciente %}
    <h1>Editar Paciente</h1>
    {% else %}
    <h1>Cargar Paciente</h1>
    {% endif %}
    <form method="post">
        {% csrf_token %}
        {{ form.as_p }}
        <button type="submit">Guardar</button>
    </form>
    <a href="{% url 'index' %}">Home</a>
</body>

</html>